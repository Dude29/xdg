{
  "name": "@folder/xdg",
  "description": "Get the XDG Base Directories or sensible defaults for Linux, Windows, or MacOS.",
  "version": "0.1.0",
  "repository": "folder/xdg",
  "homepage": "https://github.com/folder/xdg",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "bugs": {
    "url": "https://github.com/folder/xdg/issues"
  },
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "license": "MIT",
  "devDependencies": {
    "gulp-format-md": "^2.0.0"
  },
  "keywords": [
    "base",
    "basedir",
    "cache",
    "config",
    "data",
    "dir",
    "directories",
    "directory",
    "dirname",
    "dot",
    "file path",
    "file paths",
    "file",
    "files",
    "filepath",
    "filepaths",
    "find",
    "folder",
    "global",
    "home",
    "linux",
    "path",
    "paths",
    "rc",
    "runtime",
    "spec",
    "unix",
    "user",
    "xd",
    "xdg"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [
        "expand-tilde",
        "global-modules",
        "global-paths",
        "normalize-path"
      ]
    }
  }
}